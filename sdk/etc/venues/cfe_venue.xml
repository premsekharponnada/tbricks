        <venue name="Chicago Futures Exchange"
               shortname="CFE OLD"
               identifier="9ba33b92-6b46-11e5-96b0-b5870607fea0"
               description="Chicago Futures Exchange">
            <identifier>
                <key name="symbol"/>
                <key name="class"/>
                <key name="exchange"/>
